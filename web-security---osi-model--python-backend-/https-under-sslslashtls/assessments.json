     "answer": "Symmetric"
    },
    {
     "_id": "6b10693a-086a-f90d-b750-b254142d3019",
     "correct": false,
     "answer": "Asymmetric"
    },
    {
     "_id": "4c3d6f9b-5766-09ef-6098-41d4cec06d6e",
     "correct": false,
     "answer": "Both symmetric and asymmetric"
    },
    {
     "_id": "cad78d36-0405-c4bc-e2aa-0a86d31f20a7",
     "correct": false,
     "answer": "Unknown"
    }
